{
  "expo": {
    "name": "MicrobotanyApp",
    "slug": "MicrobotanyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F6DB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.fernet.microbotany"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
